<DialogPane prefHeight="300.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controle.Cadastro">
    <content>
        <GridPane hgap="5" vgap="5">
            <Label text="Nome" GridPane.columnIndex="0" GridPane.rowIndex="0">
                <GridPane.margin>
                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                </GridPane.margin></Label>
            <TextField fx:id="tfNomeAlbum" promptText="Nome..." GridPane.columnIndex="1" GridPane.rowIndex="0">
                <GridPane.margin>
                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                </GridPane.margin></TextField>
            <Label text="Ano de Lançamento" GridPane.columnIndex="2" GridPane.rowIndex="0">
                <GridPane.margin>
                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                </GridPane.margin></Label>
            <TextField fx:id="tfAnoLancamento" promptText="Ano de Lançamento..." GridPane.columnIndex="3" GridPane.rowIndex="0">
                <GridPane.margin>
                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                </GridPane.margin></TextField>
            <Label text="Nº de Música" GridPane.columnIndex="0" GridPane.rowIndex="2">
                <GridPane.margin>
                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                </GridPane.margin></Label>
            <TextField fx:id="tfNumeroDeMusicas" promptText="Total de Músicas" GridPane.columnIndex="1" GridPane.rowIndex="2">
                <GridPane.margin>
                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                </GridPane.margin></TextField>
            <Separator prefHeight="29.0" prefWidth="113.0" GridPane.rowIndex="4" GridPane.columnIndex="0"/>
            <Separator prefHeight="29.0" prefWidth="113.0" GridPane.rowIndex="4" GridPane.columnIndex="1"/>
            <Separator prefHeight="29.0" prefWidth="113.0" GridPane.rowIndex="4" GridPane.columnIndex="2"/>
            <Separator prefHeight="29.0" prefWidth="113.0" GridPane.rowIndex="4" GridPane.columnIndex="3"/>
           


        </GridPane>
    </content>
</DialogPane>
